<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Activies.HomeActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:showIn="@layout/activity_home">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="187dp"
        android:layout_height="126dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="TODO"
        android:src="@drawable/schoolbus_p"
        app:layout_constraintBottom_toTopOf="@+id/guideline20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline21" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/passageiros"
        android:layout_width="75dp"
        android:layout_height="95dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="8dp"
        android:clickable="true"
        android:src="@drawable/daughter"
        app:civ_border_color="@color/AzulTema"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toStartOf="@+id/escolas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline20"
        tools:ignore="MissingConstraints,OnClick" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="38dp"
        android:layout_height="19dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="53dp"
        android:text="Filhos"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passageiros"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/escolas"
        android:layout_width="75dp"
        android:layout_height="95dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="15dp"
        android:clickable="true"
        android:src="@drawable/college"
        app:civ_border_color="@color/AzulTema"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toStartOf="@+id/tios"
        app:layout_constraintStart_toEndOf="@+id/passageiros"
        app:layout_constraintTop_toTopOf="@+id/guideline20"
        tools:ignore="MissingConstraints,OnClick" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="50dp"
        android:layout_height="19dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:text="Escolas"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/escolas"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/tios"
        android:layout_width="75dp"
        android:layout_height="95dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        android:src="@drawable/couple"
        app:civ_border_color="@color/AzulTema"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/escolas"
        app:layout_constraintTop_toTopOf="@+id/guideline20"
        tools:ignore="MissingConstraints,OnClick" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="29dp"
        android:layout_height="19dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="48dp"
        android:text="Tios"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/tios"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/user"
        android:layout_width="75dp"
        android:layout_height="95dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        android:clickable="true"
        android:src="@drawable/usersnew"
        app:civ_border_color="@color/AzulTema"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toStartOf="@+id/maps"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints,OnClick" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="73dp"
        android:layout_height="18dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:text="UsuÃ¡rio"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/user"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/user"
        app:layout_constraintTop_toBottomOf="@+id/user"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/maps"
        android:layout_width="75dp"
        android:layout_height="95dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="15dp"
        android:clickable="true"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toStartOf="@+id/map"
        app:layout_constraintStart_toEndOf="@+id/user"
        tools:ignore="MissingConstraints,OnClick"
        tools:layout_editor_absoluteY="354dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="70dp"
        android:layout_height="18dp"
        android:layout_marginTop="12dp"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/maps"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/map"
        android:layout_width="75dp"
        android:layout_height="95dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="30dp"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/maps"
        tools:ignore="MissingConstraints,OnClick"
        tools:layout_editor_absoluteY="354dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="66dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="460dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="43dp"
        android:layout_height="66dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="17dp"
        android:onClick="logout"
        android:src="@drawable/logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.984"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/map"
        app:layout_constraintVertical_bias="0.969"
        tools:ignore="OnClick" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="242dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="57dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="56dp" />


</android.support.constraint.ConstraintLayout>